{
  "name": "browserifix-cli-example",
  "version": "0.0.1",
  "private": true,
  "description": "Browserifix CLI example",
  "eslintConfig": {
    "extends": "@info.nl/eslint-config-berlin"
  },
  "config": {
    "browserifix": {
      "bundles": {
        "beep": {
          "require": [
            "_app/foo",
            "_app/bar"
          ]
        },
        "boop": {
          "external": [
            "_app/foo",
            "_app/bar"
          ]
        }
      },
      "vendors": {
        "vendor": [
          "react"
        ]
      },
      "source": "./source/js",
      "target": "./public/js"
    }
  },
  "scripts": {
    "js": "browserifix",
    "prejs:dev": "npm run js:vendor",
    "js:dev": "browserifix watch",
    "js:vendor": "browserifix vendor",
    "js:watch": "browserifix watch"
  },
  "dependencies": {
    "@info.nl/eslint-config-berlin": "^0.1.1",
    "browserifix": "0.3.1",
    "domready": "^1.0.7",
    "react": "^0.13.3"
  }
}
