{
  "name": "@info.nl/jsxmas-cli-example",
  "version": "0.0.1",
  "private": true,
  "description": "@info.nl JSXmas CLI example",
  "eslintConfig": {
    "extends": "@info.nl/eslint-config-berlin"
  },
  "config": {
    "@info.nl/jsxmas": {
      "bundles": {
        "beep": {
          "require": [
            "_app/foo",
            "_app/bar"
          ]
        },
        "boop": {
          "external": [
            "_app/foo",
            "_app/bar"
          ]
        }
      },
      "vendors": {
        "vendor": [
          "react"
        ]
      },
      "source": "./source/js",
      "target": "./public/js"
    }
  },
  "scripts": {
    "prejs": "npm i",
    "js": "jsxmas",
    "prejs:d": "npm run js",
    "js:d": "npm run js watch"
  },
  "dependencies": {
    "@info.nl/eslint-config-berlin": "^0.1.1",
    "@info.nl/jsxmas": "1.0.0",
    "domready": "^1.0.7",
    "react": "^0.13.3"
  }
}
