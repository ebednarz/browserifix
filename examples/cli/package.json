{
  "name": "browserifix-cli-example",
  "version": "0.0.1",
  "private": true,
  "description": "Browserifix CLI example",
  "config": {
    "browserifix": {
      "bundles": {
        "beep": {
          "require": [
            "_app/foo",
            "_app/bar"
          ]
        },
        "boop": {
          "external": [
            "_app/foo",
            "_app/bar"
          ]
        }
      },
      "source": "./source/js",
      "target": "./public/js"
    }
  },
  "scripts": {
    "js": "browserifix",
    "prejs:dev": "npm run js",
    "js:dev": "browserifix watch",
    "minify": "uglifyjs public/js/beep.js --in-source-map public/js/beep.js.map --source-map-root /XXX  --source-map public/js/beep.min.js.map --source-map-url beep.min.js.map --source-map-include-sources -o public/js/beep.min.js"
  },
  "dependencies": {
    "browserifix": "0.1.0",
    "domready": "^1.0.7"
  }
}
