{
  "name": "@info.nl/jsxmas-api-example",
  "version": "0.0.1",
  "private": true,
  "description": "Info.nl JSXmas API example",
  "eslintConfig": {
    "ecmaFeatures": {
      "modules": true
    },
    "env": {
      "es6": true
    }
  },
  "config": {
    "@info.nl/jsxmas": {
      "bundles": {
        "beep": {
          "require": [
            "_app/foo",
            "_app/bar"
          ]
        },
        "boop": {
          "external": [
            "_app/foo",
            "_app/bar"
          ]
        }
      },
      "map": "$1-$2-map.json",
      "min": "$1-min.$2"
    }
  },
  "scripts": {
    "js": "gulp"
  },
  "dependencies": {
    "@info.nl/jsxmas": "1.0.0",
    "gulp": "^3.8.11"
  }
}
